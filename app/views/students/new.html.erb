<%= form_for(@student) do |f| %>
  <label>Student First Name </label>
  <%= f.text_field :first_name %>
  <label>Student Last Name </label>
  <%= f.text_field :last_name %>

  <label>Student Title </label>
  <%= f.text_field :title %>
  <label>Room Number </label>
  <%= f.text_field :room_number%>

  <%= f.submit>

<% end %>
